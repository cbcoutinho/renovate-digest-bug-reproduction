{
  "registryAliases": {
    "docker.io": "harbor.internal.coutinho.io/docker.io"
  },
  "packageRules": [
    {
      "description": "official images to docker.io",
      "matchDatasources": [
        "docker"
      ],
      "matchPackageNames": [
        "/^[a-z-]+$/"
      ],
      "replacementNameTemplate": "docker.io/library/{{{packageName}}}"
    },
    {
      "description": "non-official images to docker.io",
      "matchDatasources": [
        "docker"
      ],
      "matchPackageNames": [
        "/^[a-z-]+/[a-z-]+$/"
      ],
      "replacementNameTemplate": "docker.io/{{ `{{{packageName}}}` }}"
    }
  ]
}
