{
  "registryAliases": {
    "docker.io": "public.ecr.aws/docker"
  },
  "packageRules": [
    {
      "description": "official images to docker.io",
      "matchDatasources": ["docker"],
      "matchPackageNames": ["/^[a-z-]+$/"],
      "replacementNameTemplate": "docker.io/library/{{{packageName}}}"
    }
  ]
}
